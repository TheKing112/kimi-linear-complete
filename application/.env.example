# ✅ ERSTELLEN - Template für sichere Konfiguration
# =============================================================================
# KIMI LINEAR 48B - ENVIRONMENT TEMPLATE
# Kopiere diese Datei zu .env und fülle die Werte aus
# =============================================================================

# Discord Bot (REQUIRED)
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=
DISCORD_OWNER_ID=your_discord_user_id
DISCORD_CONTROL_USERS=comma,separated,user,ids
DISCORD_CONTROL_CHANNEL=channel_id_for_status

# Kimi Linear Model
MODEL_NAME=moonshotai/Kimi-Linear-48B-A3B-Instruct
MODEL_PATH=/models/kimi-linear-48b
DEVICE=cuda
LOAD_IN_4BIT=true
TORCH_COMPILE=true
TRUST_REMOTE_CODE=true
MAX_INPUT_LENGTH=1048576
GPU_COUNT=1
CUDA_VISIBLE_DEVICES=0

# Hugging Face (Optional for private models)
HF_TOKEN=

# Database (Auto-generated passwords - DO NOT EDIT)
POSTGRES_PASSWORD=WILL_BE_GENERATED
POSTGRES_USER=cognee
POSTGRES_DB=cognee
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis
REDIS_URL=redis://redis:6379/0

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_WEBHOOK_SECRET=WILL_BE_GENERATED
GITHUB_AUTO_REVIEW=true
GITHUB_AUTO_CREATE_PR=false

# Autonomous Editing
AUTONOMOUS_MODE_ENABLED=true
AUTONOMOUS_REQUIRE_APPROVAL=true
AUTONOMOUS_MAX_TOKENS=4096
AUTONOMOUS_TEMPERATURE=0.2

# Optional AI APIs
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Monitoring
GRAFANA_PASSWORD=WILL_BE_GENERATED

# Network
NETWORK_NAME=kimi-network
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8003

# Rate Limiting
API_RATE_LIMIT_PER_USER=10/minute
API_RATE_LIMIT_PER_IP=100/minute

# Logging
LOG_LEVEL=INFO